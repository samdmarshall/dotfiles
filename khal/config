[calendars]

[[Personal]]
path = ~/Calendars/fastmail/Personal/*
type = discover

[[Medical]]
path = ~/Calendars/fastmail/Medical/*
type = discover

[[Work]]
path = ~/Calendars/fastmail/Work/*
type = discover

[[Cocoaheads]]
path = ~/Calendars/fastmail/Cocoaheads/*
type = discover
readonly = True

[[Games]]
path = ~/Calendars/fastmail/Games/*/*
type = discover
readonly = True

[[Holidays]]
path = ~/Calendars/fastmail/Holidays/*
type = discover
readonly = True

[[Birthdays]]
path = ~/Contacts/Default/
type = birthdays
color = light gray

