[default]
highlight_event_days = true
default_calendar = "MLB"
default_command = "interactive"

[calendars]
[[MLB]]
path = ~/Calendars/MLB
type = discover
color = dark red
[[US Holidays]]
path = ~/Calendars/us-holiday
type = discover
color = yellow
readonly = True

[locale]
timeformat = %H:%M
dateformat = %m.%d.%Y
longdateformat = %m.%d.%Y
datetimeformat = %m.%d.%Y %H:%M
longdatetimeformat = %m.%d.%Y %H:%M

[view]
#frame = True
frame = color
theme = light

