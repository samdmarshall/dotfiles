[general]
status_path = "~/.config/vdirsyncer/status"

[pair mlb]
a = "local_mlb"
b = "remote_mlb"
collections = null
metadata = ["color", "displayname"]

[storage local_mlb]
type = "filesystem"
path = "~/Calendars/MLB"
fileext = ".ics"

[storage remote_mlb]
type = "http"
url.fetch = ["command", "/usr/local/bin/secure-env", "get", "--key:vdirsyncer-work-calendar"]

[pair us_holidays]
a = "us_holiday_remote"
b = "us_holiday_local"
collections = null

[storage us_holiday_remote]
type = "http"
url = "https://p09-calendars.icloud.com/holidays/us_en.ics"
read_only = "true"

[storage us_holiday_local]
type = "filesystem"
path = "~/Calendars/us-holiday"
fileext = ".ics"
