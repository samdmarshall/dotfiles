[general]
status_path = "~/.config/vdirsyncer/status"

[pair icloud]
a = "local_icloud"
b = "remote_icloud"
collections = ["from a", "from b"]
conflict_resolution = "a wins"
metadata = ["color", "displayname"]

[storage local_icloud]
type = "filesystem"
path = "~/Calendars/icloud"
fileext = ".ics"

[storage remote_icloud]
type = "caldav"
url = "https://caldav.icloud.com/"
username.fetch = ["command", "/usr/local/bin/rune", "get", "--key:vdirsyncer-icloud-username"]
password.fetch = ["command", "/usr/local/bin/rune", "get", "--key:vdirsyncer-icloud-password"]


[pair cocoaheads]
a = "remote_cocoaheads"
b = "local_cocoaheads"
collections = null

[storage local_cocoaheads]
type = "filesystem"
path = "~/Calendars/cocoaheads"
fileext = ".ics"

[storage remote_cocoaheads]
type = "http"
url = "http://www.cocoaheads.org//us/BostonMassachusetts/events.ics"
read_only = "true"


[pair ellen]
a = "remote_ellen"
b = "local_ellen"
collections = null

[storage local_ellen]
type = "filesystem"
path = "~/Calendars/ellen"
fileext = ".ics"

[storage remote_ellen]
type = "http"
url.fetch = ["command", "/usr/local/bin/rune", "get", "--key:vdirsyncer-ellen-calendar"]
read_only = "true"


[pair us_holidays]
a = "us_holiday_remote"
b = "us_holiday_local"
collections = null

[storage us_holiday_remote]
type = "http"
url = "https://p09-calendars.icloud.com/holidays/us_en.ics"
read_only = "true"

[storage us_holiday_local]
type = "filesystem"
path = "~/Calendars/us-holiday"
fileext = ".ics"


[pair birthdays]
a = "birthdays_ical"
b = "birthdays_khal"
collections = null

[storage birthdays_ical]
type = "filesystem"
path = "~/Library/Calendars/BCD5BB2D-2DD8-4F92-A8DE-935F5453C262.calendar/Events"
fileext = ".ics"

[storage birthdays_khal]
type = "filesystem"
path = "~/Calendars/birthdays"
fileext = ".ics"


[pair task_list]
a = "task_list_local"
b = "task_list_remote"
collections = ["from a", "from b"]
conflict_resolution = "a wins"
metadata = ["color", "displayname"]

[storage task_list_local]
type = "filesystem"
path = "~/Tasks/primary"
fileext = ".ics"

[storage task_list_remote]
type = "caldav"
url.fetch = ["command", "/usr/local/bin/rune", "get", "--key:vdirsyncer-task-list-url"]
username.fetch = ["command", "/usr/local/bin/rune", "get", "--key:vdirsyncer-task-list-username"]
password.fetch = ["command", "/usr/local/bin/rune", "get", "--key:vdirsyncer-task-list-password"]


[pair contacts_list]
a = "address_book_local"
b = "address_book_remote"
collections = ["from a", "from b"]
conflict_resolution = "a wins"

[storage address_book_local]
type = "filesystem"
path = "~/Contacts/"
fileext = ".vcf"

[storage address_book_remote]
type = "carddav"
url = "https://contacts.icloud.com/"
username.fetch = ["command", "/usr/local/bin/rune", "get", "--key:vdirsyncer-icloud-username"]
password.fetch = ["command", "/usr/local/bin/rune", "get", "--key:vdirsyncer-icloud-password"]

