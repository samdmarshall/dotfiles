b = "chronicle_khal"
collections = null

[storage chronicle_ical]
type = "filesystem"
path = "~/Library/Calendars/019AA509-13C1-4887-BB5F-BCD632068918.calendar/Events"
fileext = ".ics"

[storage chronicle_khal]
type = "filesystem"
path = "~/Calendars/chronicle"
