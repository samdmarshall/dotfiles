[general]
status_path = "~/.config/vdirsyncer/status"

[pair cocoaheads]
a = "remote_cocoaheads"
b = "local_cocoaheads"
collections = null

[storage local_cocoaheads]
type = "filesystem"
path = "~/Calendars/cocoaheads"
fileext = ".ics"

[storage remote_cocoaheads]
type = "http"
url = "http://www.cocoaheads.org//us/BostonMassachusetts/events.ics"
read_only = "true"


[pair ellen]
a = "remote_ellen"
b = "local_ellen"
collections = null

[storage local_ellen]
type = "filesystem"
path = "~/Calendars/ellen"
fileext = ".ics"

[storage remote_ellen]
type = "http"
url.fetch = ["command", "/home/samantha/.local/bin/rune", "get", "--key:vdirsyncer-ellen-calendar"]
read_only = "true"

[pair us_holidays]
a = "us_holiday_remote"
b = "us_holiday_local"
collections = null

[storage us_holiday_remote]
type = "http"
url = "https://p09-calendars.icloud.com/holidays/us_en.ics"
read_only = "true"

[storage us_holiday_local]
type = "filesystem"
path = "~/Calendars/us-holiday"
fileext = ".ics"

[pair fastmail]
a = "fastmail_calendar_local"
b = "fastmail_calendar_remote"
# DE44DE89-5BEF-4B6A-AD13-15F9D0C4D2D8 == Personal
# 94ff4eb9bc104fdfa44b2e1acd03ba8b 		 == Work Events
# 5f96f604-51c0-4db7-a0ec-2eb60e27f300 == Deliveries
