{
	"name": "python-langauge-server",
	"description": "Python implementation of a language server in node.",
	"version": "0.0.1",
	"author": "Thomas Haakon Townsend",
	"license": "MIT",
	"engines": {
		"node": "*"
	},
	"dependencies": {
		"vscode-languageserver": "^1.3.0"
	},
	"devDependencies": {
		"typescript": "^1.7.5"
	},
	"scripts": {
		"compile": "installServerIntoExtension ../../out ./package.json ./tsconfig.json && tsc -p .",
		"watch": "installServerIntoExtension ../../out ./package.json ./tsconfig.json && tsc --watch -p ."
	}
}