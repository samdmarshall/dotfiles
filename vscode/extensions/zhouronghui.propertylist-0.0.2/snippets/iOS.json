{
    "CoreSpotlightContinuation": {
        "prefix": "CoreSpotlightContinuation",
        "body": [
            "<key>CoreSpotlightContinuation</key>",
            "<${1|true,false|}/>"
        ],
        "description": "Specifies whether the app supports Spotlight query continuation. See CoreSpotlightContinuation for details."
    },
    "INAlternativeAppNames": {
        "prefix": "INAlternativeAppNames",
        "body": [
            "<key>INAlternativeAppNames</key>",
            "<array>",
            "\t<dict>",
			"\t\t<key>INAlternativeAppName</key>",
			"\t\t<string>${1:}</string>",
			"\t\t<key>INAlternativeAppNamePronunciationHint</key>",
			"\t\t<string>${2:}</string>",
		    "\t</dict>",
            "</array>"
        ],
        "description": "Specifies alternative names by which users can refer to the app in Siri. See INAlternativeAppNames for details."
    },
    "MKDirectionsApplicationSupportedModes": {
        "prefix": "MKDirectionsApplicationSupportedModes",
        "body": [
            "<key>MKDirectionsApplicationSupportedModes</key>",
