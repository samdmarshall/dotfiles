{
	"name": "graphviz-preview",
	"version": "1.3.0",
	"publisher": "EFanZh",
	"engines": {
		"vscode": "^1.26.1"
	},
	"license": "SEE LICENSE IN LICENSE",
	"displayName": "Graphviz Preview",
	"description": "Preview Graphviz (DOT) files.",
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"dot",
		"graphviz",
		"preview"
	],
	"main": "./out/extension",
	"contributes": {
		"languages": [
			{
				"id": "dot",
				"aliases": [
					"Graphviz",
					"DOT"
				],
				"extensions": [
					".dot",
					".gv"
				]
			}
		],
		"commands": [
			{
				"command": "graphviz.showPreview",
				"title": "Show Preview",
				"category": "Graphviz",
				"icon": {
					"dark": "resources/show-preview-dark.svg",
					"light": "resources/show-preview-light.svg"
				}
			}
		],
		"configuration": {
			"title": "Graphviz Preview",
			"properties": {
				"graphvizPreview.dotPath": {
					"description": "The path of the dot program.",
					"type": [
						"string",
						"null"
					],
					"default": null
				},
				"graphvizPreview.engine": {
					"description": "The layout engine to use.",
					"type": [
						"string",
						"null"
					],
					"enum": [
						"dot",
						null
					],
					"default": null
				}
			}
		},
		"menus": {
			"editor/title": [
				{
					"command": "graphviz.showPreview",
					"group": "navigation",
					"when": "editorLangId == dot"
				}
			]
		}
	},
	"activationEvents": [
		"onCommand:graphviz.showPreview"
	],
	"devDependencies": {
		"@types/mocha": "^5.2.5",
		"@types/node": "^10.7.1",
		"concurrently": "^3.6.1",
		"tslint": "^5.11.0",
		"typescript": "^3.0.1",
		"vscode": "^1.1.21",
		"webpack": "^4.17.1",
		"webpack-cli": "^3.1.0"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p . && webpack",
		"watch": "concurrently --kill-others --allow-restart --restart-after 500 -r \"tsc -p . -w\" \"webpack -w\"",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"lint": "tslint -p .",
		"test": "npm run lint && npm run compile && node ./node_modules/vscode/bin/test"
	},
	"icon": "resources/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/EFanZh/Graphviz-Preview"
	},
	"__metadata": {
		"id": "b3aaaed0-c0e2-4e03-9121-e6faa9d7297e",
		"publisherId": "727d81ac-090a-4218-8947-960e102dc001",
		"publisherDisplayName": "EFanZh"
	}
}