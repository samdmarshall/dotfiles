{
	"name": "Hg",
	"displayName": "Hg - Mercurial",
	"description": "Basic mercurial support for VSCode",
	"version": "0.0.4",
	"publisher": "jake-coltman",
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.commit",
		"onCommand:extension.push",
		"onCommand:extension.pull"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.commit",
				"title": "Hg - Commit"
			},
			{
				"command": "extension.push",
				"title": "Hg - Push"
			},
			{
				"command": "extension.pull",
				"title": "Hg - Pull"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"__metadata": {
		"id": "6923c83d-1fc0-44b7-9233-e8b2392152cc",
		"publisherId": "6dce1c7a-e688-4845-a558-c2dd10e100fc",
		"publisherDisplayName": "Jake Coltman"
	}
}